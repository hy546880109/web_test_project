eval("__webpack_require__.r(__webpack_exports__);\n/* harmony export (binding) */ __webpack_require__.d(__webpack_exports__, \"setLabelText\", function() { return setLabelText; });\n/* harmony export (binding) */ __webpack_require__.d(__webpack_exports__, \"setLabelStyle\", function() { return setLabelStyle; });\n/* harmony export (binding) */ __webpack_require__.d(__webpack_exports__, \"getLabelStatesModels\", function() { return getLabelStatesModels; });\n/* harmony export (binding) */ __webpack_require__.d(__webpack_exports__, \"createTextStyle\", function() { return createTextStyle; });\n/* harmony export (binding) */ __webpack_require__.d(__webpack_exports__, \"createTextConfig\", function() { return createTextConfig; });\n/* harmony export (binding) */ __webpack_require__.d(__webpack_exports__, \"getFont\", function() { return getFont; });\n/* harmony export (binding) */ __webpack_require__.d(__webpack_exports__, \"labelInner\", function() { return labelInner; });\n/* harmony export (binding) */ __webpack_require__.d(__webpack_exports__, \"setLabelValueAnimation\", function() { return setLabelValueAnimation; });\n/* harmony export (binding) */ __webpack_require__.d(__webpack_exports__, \"animateLabelValue\", function() { return animateLabelValue; });\n/* harmony import */ var zrender_lib_graphic_Text__WEBPACK_IMPORTED_MODULE_0__ = __webpack_require__(/*! zrender/lib/graphic/Text */ \"./node_modules/zrender/lib/graphic/Text.js\");\n/* harmony import */ var zrender_lib_core_util__WEBPACK_IMPORTED_MODULE_1__ = __webpack_require__(/*! zrender/lib/core/util */ \"./node_modules/zrender/lib/core/util.js\");\n/* harmony import */ var _util_states__WEBPACK_IMPORTED_MODULE_2__ = __webpack_require__(/*! ../util/states */ \"./node_modules/echarts/lib/util/states.js\");\n/* harmony import */ var _util_log__WEBPACK_IMPORTED_MODULE_3__ = __webpack_require__(/*! ../util/log */ \"./node_modules/echarts/lib/util/log.js\");\n/* harmony import */ var _util_model__WEBPACK_IMPORTED